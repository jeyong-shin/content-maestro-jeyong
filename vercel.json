{
    "functions": {
      "api/contents/generate": {
        "maxDuration": 60
      },
      "app/api/contents/generate/route": {
        "maxDuration": 60
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }