{
    "functions": {
      "api/**/*.js": {
        "maxDuration": 60
      },
      ".next/server/app/api/**/*.js": {
        "maxDuration": 60
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }